           "PA3", "PA4", "PA5", "PA6",
           "PA7", "PA8", "PA9", "PA10",
           "PA11", "PA12", "PA13", "PA14",
           "PA15", "PA16";
    function = "emac";
   };

   gmac_pins_mii_a: gmac_mii@0 {
    pins = "PA0", "PA1", "PA2",
           "PA3", "PA4", "PA5", "PA6",
           "PA7", "PA8", "PA9", "PA10",
           "PA11", "PA12", "PA13", "PA14",
           "PA15", "PA16";
    function = "gmac";
   };

