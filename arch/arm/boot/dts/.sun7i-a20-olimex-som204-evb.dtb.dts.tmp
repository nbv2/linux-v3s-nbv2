   interrupt-controller;
   #interrupt-cells = <3>;
   #gpio-cells = <3>;

   can0_pins_a: can0@0 {
    pins = "PH20", "PH21";
    function = "can";
   };

   clk_out_a_pins_a: clk_out_a@0 {
