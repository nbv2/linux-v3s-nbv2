   clock-names = "ahb",
          "mmc",
          "output",
          "sample";
   interrupts = <0 35 4>;
   status = "disabled";
   #address-cells = <1>;
   #size-cells = <0>;
  };

  usb_otg: usb@1c13000 {
   compatible = "allwinner,sun4i-a10-musb";
   reg = <0x01c13000 0x0400>;
   clocks = <&ccu 26>;
   interrupts = <0 38 4>;
   interrupt-names = "mc";
   phys = <&usbphy 0>;
   phy-names = "usb";
   extcon = <&usbphy 0>;
   allwinner,sram = <&otg_sram 1>;
   status = "disabled";
  };

  usbphy: phy@1c13400 {
   #phy-cells = <1>;
   compatible = "allwinner,sun7i-a20-usb-phy";
   reg = <0x01c13400 0x10 0x01c14800 0x4 0x01c1c800 0x4>;
   reg-names = "phy_ctrl", "pmu1", "pmu2";
   clocks = <&ccu 125>;
   clock-names = "usb_phy";
   resets = <&ccu 1>,
     <&ccu 2>,
     <&ccu 3>;
   reset-names = "usb0_reset", "usb1_reset", "usb2_reset";
   status = "disabled";
  };

  ehci0: usb@1c14000 {
   compatible = "allwinner,sun7i-a20-ehci", "generic-ehci";
   reg = <0x01c14000 0x100>;
   interrupts = <0 39 4>;
   clocks = <&ccu 27>;
   phys = <&usbphy 1>;
