  pio: pinctrl@1c20800 {
   compatible = "allwinner,sun7i-a20-pinctrl";
   reg = <0x01c20800 0x400>;
   interrupts = <0 28 4>;
   clocks = <&ccu 74>, <&osc24M>, <&osc32k>;
   clock-names = "apb", "hosc", "losc";
