   dmas = <&dma 1 14>;

   ports {
    #address-cells = <1>;
    #size-cells = <0>;

    tcon0_in: port@0 {
     #address-cells = <1>;
     #size-cells = <0>;
     reg = <0>;

     tcon0_in_be0: endpoint@0 {
      reg = <0>;
      remote-endpoint = <&be0_out_tcon0>;
     };

     tcon0_in_be1: endpoint@1 {
      reg = <1>;
      remote-endpoint = <&be1_out_tcon0>;
